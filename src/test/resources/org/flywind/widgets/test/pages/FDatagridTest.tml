<form class="form-inline" t:type="form" role="form" t:zone="zoneOne" t:autofocus="false">
			<div class="form-group">
				<label class="control-label">用户名<span class="mr5">:</span></label>
				<input class="iw120 form-control" type="text" placeholder="输入用户名" t:type="textfield" t:id="userName"/>
			</div>
			<div class="form-group">
				<button class="btn btn-block btn-info btn-flat">搜索</button>
			</div>
</form>
<div class="btn-box">
	         <div class="btn-group mr10">
	           <t:widgets.fdatagridbtn t:clientId="edit" linkType="page" text="编辑" url="FDatagridModeTest" gridId="newContracts"/>
	         </div>
	         <div class="btn-group mr10">
	           <t:widgets.fdatagridbtn t:clientId="edit5" linkType="page" paramsMode="true" text="编辑" url="FDatagridTest4" gridId="newContracts"/>
	         </div>
	         <div class="btn-group mr10">
	         	<t:widgets.fdatagridbtn t:clientId="delete" zone="zoneOne"  text="删除" url="${delUrl}" alertMsg="没有选择" gridId="newContracts" />
	         </div>
</div>
<t:zone t:id="zoneOne" id="zoneOne" style="height:500px">
<table t:type="widgets/fdatagrid" t:id="newContracts" t:source="examples" t:pageSize="20">
	<thead>
		<tr>
			<th data-options="field:'id',width:80,title:'编号',sortable:true"></th>
			<th data-options='field:"creater",width:300,title:"创建者",
							  formatter:function(value,row){
							  	if(row.creater == "kitty"){
							  		
							  		return row.creater+" &lt;a&gt;htttt&lt;/a&gt;";
							  	}else{
							  		return row.creater+" world";
							  	}
							  }'></th>
		    <th data-options="field:'customerCode',width:80,title:'客户代码'"></th>
		    <th data-options="field:'userName',width:80,title:'${message:userName-label}'"></th>
		    <th data-options="field:'createTime',width:150,title:'创建时间'"></th>
		    <th data-options="field:'lastUpdateTime',width:150,title:'更新时间'"></th>
		    <th data-options="field:'lastUpdatePerson',width:80,title:'更新人'"></th>
		</tr>
	</thead>
</table>
</t:zone>
<div class="btn-box">
	<div class="btn-group mr10"><t:widgets.fdatagridbtn t:clientId="editT" linkType="page"  text="编辑" url="FDatagridTest2" gridId="newContracts2"/></div>
	<div class="btn-group mr10"><t:widgets.fdatagridbtn t:clientId="deleteT" zone="zoneTwo"  text="删除" url="${deleteUrl}" alertMsg="没有选择任何数据" gridId="newContracts2" /></div>
	<div class="btn-group mr10"><t:widgets.FDialogGridLink t:clientId="editTwo333" zone="zoneTwo" text="dialog查询" url="${deleteUrl}" dialog="medialog" gridId="newContracts2"/></div>
</div>
<t:zone t:id="zoneTwo" id="zoneTwo" style="height:500px;">
<table t:type="widgets/fdatagrid" t:id="newContracts2" t:source="examples">
	<thead>
		<tr>
			<th data-options="field:'userName',width:80,title:'用户名',sortable:true"></th>
			<th data-options="field:'creater',width:80,title:'创建者'"></th>
		</tr>
	</thead>
</table>
</t:zone>
<t:widgets.fdialog t:clientId="medialog" t:params="{'width':1000,'height':400}">
	<table t:type="widgets/fdatagrid" t:id="newContracts3" t:source="examples" t:params="{'singleSelect':false,'checkOnSelect':true,'selectOnCheck':true}" t:singleSelect="false">
		<thead>
			<tr>
				<th data-options="field:'ck','checkbox':true"></th>
				<th data-options="field:'userName',width:80,title:'用户名'"></th>
				<th data-options="field:'creater',width:80,title:'创建者'"></th>
			</tr>
		</thead>
	</table>
</t:widgets.fdialog>
</html>
